@using EatMeat.Database.Entities
@model List<MeatEntity>

<div  class="main-content-wrapper">
    <div class="shop-page-wrapper shop-fullwidth ptb--80">
        <div class="container shop-products list">
            <div class="row">
                @if(Model.Count == 0)
                {
                    <label>No one product</label>
                }
                else
                {
                    @foreach(MeatEntity meat in Model)
                    {
                        <partial name="_MeatItemPartial" model="meat" />
                    }
                }
            </div>
        </div>
    </div>
</div>